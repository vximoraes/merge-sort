  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vximoraes/merge-sort/issues"
  },
  "homepage": "https://github.com/vximoraes/merge-sort#readme",
  "dependencies": {
    "tslab": "^1.0.22",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
