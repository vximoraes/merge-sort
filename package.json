{
  "name": "merge-sort",
  "version": "1.0.0",
  "description": "Projeto de algoritmo merge sort com Jupyter Notebook TypeScript",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && npm run install-kernel",
  "install-kernel": "bash ./scripts/install_tslab.sh",
  "start": "bash ./scripts/start_jupyter.sh --no-browser --port=8888",
  "start-browser": "bash ./scripts/start_jupyter.sh",
  "lab": "bash ./scripts/start_jupyter.sh",
    "kernel-list": "python3 -m jupyter kernelspec list",
    "kernel-remove": "python3 -m jupyter kernelspec remove tslab",
    "check": "node --version && python3 -m jupyter --version && python3 -m jupyter kernelspec list | grep tslab",
    "clean": "npm run kernel-remove && npm install && npm run install-kernel",
    "test": "echo \"âœ… Para testar, execute 'npm start' e abra src/teste.ipynb\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vximoraes/merge-sort.git"
  },
  "keywords": [],
